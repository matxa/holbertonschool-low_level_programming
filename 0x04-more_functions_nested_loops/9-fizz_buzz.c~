#include "holberton.h"
#include <stdio.h>

/**
 * Fizz milti-3
 * Buzz muilti-5
 * FizzBuzz multi 3 & 5
 * main - print
 * Return: Always 0 (Success)
 */

int main(void)
{
	int n;

	for (n = 1; n <= 100; n++)
	{
		if (n % 3 != 0 && n % 5 != 0)
		{
			printf("%d ", n);
		}
		if (n % 3 == 0 && n % 5 == 0)
		{
			printf("FizzBuzz ");
		}
		else if (n % 5 == 0)
		{
			printf("Buzz ");
		}
		else if (n % 3 == 0)
		{
			printf("Fizz ");
		}
	}
	putchar('\n');
	return (0);
}

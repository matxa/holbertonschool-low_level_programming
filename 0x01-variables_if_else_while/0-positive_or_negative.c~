#include <stdlib.h>
#include <time.h>
/* more headers goes there */
/* This determines if the output is a positive, negative or zero*/
/* betty style doc for function main goes there */
int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	if (n > 0)
	{
		printf("%d is a positive number ", n);
	}
	else if (n < 0)
	{
		printf("%d is a negative number ", n);
	}
	else
	{
		printf("%d is zero ", n);
	}
	return (0);
}

#include "holberton.h"

/**
 **_strncat - copy
 *@dest: pointer
 *@src: pointer
 *Return: (dest)
 */

char *_strcat(char *dest, char *src)
{
	int i;
	char *cat = dest;

	while (*dest != '\0')
	{
		dest++;
	}
	for (i = 0; src[i] != '\0'; i++)
	{
		*dest++ = *src++;
	}
	dest[i] = '\0';

	return (cat);
}
